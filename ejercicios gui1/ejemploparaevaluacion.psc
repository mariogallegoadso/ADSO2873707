Proceso ejemploparaevaluacion
	// declaracion/definicion 
	definir alto,ancho,area,perimetro como real;
	// entrada 
	Escribir "digite el alto del rectangulo";
	leer alto;
	escribir "digite el ancho del rectangulo";
	leer ancho;
	//proceso y formulas- logica
	area <- alto*ancho;
	perimetro <- alto*2+ancho*2;
	// salida 
	escribir "area del perimetro es:",area;
	escribir "el perimetro del rectangulo es:",perimetro;
	
	
FinProceso
