Proceso ejercicio20
	definir a,b como real;
	escribir "escriba el primer numero:";
	leer a;
	escribir "escriba el segundo numero:";
	leer b;
	si (a==b) Entonces
		escribir "los numeros son iguales:";
	FinSi 
		si (a<b) entonces 
		escribir "el primer numero es menor que el segundo:";
	SiNo
